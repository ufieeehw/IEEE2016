<launch>
  <group ns="camera">
    <node pkg="libuvc_camera" type="camera_node" name="cam_1">
      <!-- Parameters used to find the camera -->
      <param name="vendor" value="0x046d"/>
      <param name="product" value="0x082d"/>
      <param name="serial" value="57E86DAF"/>

      <!-- Image size and type -->
      <param name="width" value="640"/>
      <param name="height" value="480"/>
      <!-- choose whichever uncompressed format the camera supports: -->
      <param name="video_mode" value="yuyv"/> <!-- or yuyv/nv12/jpeg -->
      <param name="frame_rate" value="30"/>

      <param name="timestamp_method" value="start"/> <!-- start of frame -->
      <param name="frame_id" value="cam_1"/>
      <param name="camera_info_url" value="package://ieee2016_vision/calibrations/$cam_1.yaml"/>

      <param name="auto_exposure" value="0"/> <!-- use aperture_priority auto exposure -->
      <param name="auto_focus" value="False"/>
      <param name="focus_absolute" value="10"/> <!--mm-->
      <param name="auto_white_balance" value="False"/>
    </node>
  </group>
</launch>